<!--pages/students/students.wxml-->
<view class="container">
	<view class="swiper">
		<view class="swiper-tab">
			<view class="swiper-tab-list {{currentTab==0 ? 'on' : ''}}" data-current="0" bindtap="swichNav">已报名学生</view>
			<view class="swiper-tab-list {{currentTab==1 ? 'on' : ''}}" data-current="1" bindtap="swichNav">预约学生</view>
		</view>
		<swiper current="{{currentTab}}" class="swiper-box" duration="300" bindchange="bindChange">
			<swiper-item>
      <view style="width:100%;height:500rpx">
      <label>1</label>
        	<label>1</label>
          	<label>1</label>
            	<label>1</label>
      </view>
			</swiper-item>
			<swiper-item>
      <view>
      <view style='padding : 10rpx;'>
					<etable config="{{config}}"></etable>
				</view>
      </view>
				
			</swiper-item>
		</swiper>
	</view>
</view>